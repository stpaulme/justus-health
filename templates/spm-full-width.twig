{% extends "base.twig" %}

{% block content %}
	{% if content|length > 0 %}
		{% for module in content %}
			{% set module_name = module.acf_fc_layout|replace({'_': '-'}) %}
			{% set module_style = module.style|replace({'_': '-'}) %}
			{% include ['blocks/module-' ~ module_name ~ '.twig', 'blocks/module.twig'] %}
		{% endfor %}
	{% endif %}
{% endblock %}
